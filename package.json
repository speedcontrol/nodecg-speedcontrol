    "eslint-plugin-vue": "^8.7.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "globby": "^13.2.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodecg-cli": "^8.6.8",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32",
