{
  "name": "nodecg-speedcontrol",
  "version": "0.9.0",
  "homepage": "http://twitch.tv/sethcharleon",
  "author": "Tobias Palm Nerg <charleon@gmail.com>",
  "contributors": [
    "btrim <btrim.twitch@gmail.com>"
  ],
  "dependencies": {
    "async": "^2.4.1",
    "clone": "^2.1.1",
    "inherits": "^2.0.1",
    "livesplit-core": "^0.9.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "needle": "^1.6.0",
    "request": "^2.73.0",
    "tmi.js": "^1.1.1",
    "ws": "^3.0.0"
  },
  "description": "NodeCG Overlay engine for Speedrun Marathons",
  "license": "MIT",
  "nodecg": {
    "compatibleRange": "~0.9.0",
    "graphics": [],
    "dashboardPanels": [
      {
        "name": "run-player",
        "title": "Run Player",
        "width": 3,
        "file": "RunPlayerDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "run-control",
        "title": "Run Editor",
        "width": 3,
        "file": "RunControlDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Run Modifications"
      },
      {
        "name": "player-control",
        "title": "Add/Remove Runs",
        "width": 2,
        "file": "PlayerControlDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Run Modifications"
      },
      {
        "name": "run-timer",
        "title": "Run Timer",
        "width": 2,
        "file": "PlayerTimerDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "player-layout",
        "title": "Player Layout",
        "width": 2,
        "file": "PlayerLayoutDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Scene Editing"
      },
      {
        "name": "scene-layout",
        "title": "Scene Layout",
        "width": 2,
        "file": "SceneLayoutDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Scene Editing"
      },
      {
        "name": "stream-control",
        "title": "Twitch Control",
        "width": 2,
        "file": "StreamControlDashboard.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "twitch-highlighting",
        "title": "Twitch Highlighting",
        "width": 2,
        "file": "TwitchHighlighting.html",
        "headerColor": "#008BB9"
      },
      {
        "name": "horaro-import",
        "title": "Horaro Schedule Import",
        "width": 2,
        "file": "HoraroImportDashboard.html",
        "headerColor": "#008BB9",
        "workspace": "Run Modifications"
      },
      {
        "name": "run-info",
        "title": "Add/Edit Run Info",
        "width": 5,
        "file": "RunInfoDialog.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "add/edit run",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ]
      }
    ]
  }
}
